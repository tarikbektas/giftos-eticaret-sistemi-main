<% if (erorrs) { %>
    <% erorrs.forEach(function(erorrsitems) { %>
        <div class="alert alert-danger"><%=erorrsitems.message  %></div>
    <% }); %>
    
    
  
<section class="">
 
    <div class="px-4 py-5 px-md-5 text-center text-lg-start" style="background-color: hsl(0, 0%, 96%)">
      <div class="container">
        <div class="row gx-lg-5 align-items-center">
          <div class="col-lg-6 mb-5 mb-lg-0">
            <h1 class="my-5 display-3 fw-bold ls-tight">
              
              <span class="text-primary">WTB E-Ticaret Sistemi</span>
            </h1>
            <p style="color: hsl(217, 10%, 50.8%)">
                WTB E-Ticaret Sistemi giriş ekranı
            </p>
          </div>
  
          <div class="col-lg-6 mb-5 mb-lg-0">
            <div class="card">
              <h1>WTB E-STORE</h1>
              <div class="card-body py-5 px-md-5">
                <form action="register" method="post">
                  
                  <div class="row">
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <input type="text"   class="form-control"  name="name"  value="<%name  %>" />
                        <label class="form-label" for="form3Example1">İsim</label>
                      </div>
                    </div>
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <input type="text"  class="form-control" name="lastname"  value="<%lastname  %>" />
                        <label class="form-label" for="form3Example2">Soyisim</label>
                      </div>
                    </div>
                  </div>
                  
                  
                     <div class="form-outline mb-4">
                      <input type="text"    class="form-control" name="phone"  value="<%phone  %>"/>
                      <label class="form-label"  > Telefon Numarası</label>
                    </div>

                   <div class="form-outline mb-4">
                    <input type="email"   class="form-control" name="email"  value="<%email  %>"   />
                    <label class="form-label"  >Email Adresi</label>
                  </div>
  
                   <div class="form-outline mb-4">
                    <input type="password"   class="form-control"  name="password"  value="<%password  %>" />
                    <label class="form-label"  >Şifre</label>
                  </div>
  
                 
  
                   <button type="submit" class="btn btn-primary btn-block mb-4">
                    kayıt
                  </button>
  
           
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
   </section>
    <% } else { %>
         
<section class="">
 
    <div class="px-4 py-5 px-md-5 text-center text-lg-start" style="background-color: hsl(0, 0%, 96%)">
      <div class="container">
        <div class="row gx-lg-5 align-items-center">
          <div class="col-lg-6 mb-5 mb-lg-0">
            <h1 class="my-5 display-3 fw-bold ls-tight">
              
              <span class="text-primary">WTB E-Ticaret Sistemi</span>
            </h1>
            <p style="color: hsl(217, 10%, 50.8%)">
                WTB E-Ticaret Sistemi giriş ekranı
            </p>
          </div>
  
          <div class="col-lg-6 mb-5 mb-lg-0">
            <div class="card">
              <h1>WTB E-STORE</h1>
              <div class="card-body py-5 px-md-5">
                <form action="register" method="post">
                  
                  <div class="row">
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <input type="text"   class="form-control"  name="name"  />
                        <label class="form-label" for="form3Example1">İsim</label>
                      </div>
                    </div>
                    <div class="col-md-6 mb-4">
                      <div class="form-outline">
                        <input type="text"  class="form-control" name="lastname"   />
                        <label class="form-label" for="form3Example2">Soyisim</label>
                      </div>
                    </div>
                  </div>
                  
                  
                     <div class="form-outline mb-4">
                      <input type="text"    class="form-control" name="phone" />
                      <label class="form-label"  > Telefon Numarası</label>
                    </div>

                   <div class="form-outline mb-4">
                    <input type="email"   class="form-control" name="email"   />
                    <label class="form-label"  >Email Adresi</label>
                  </div>
  
                   <div class="form-outline mb-4">
                    <input type="password"   class="form-control"  name="password"  />
                    <label class="form-label"  >Şifre</label>
                  </div>
  
                 
  
                   <button type="submit" class="btn btn-primary btn-block mb-4">
                    kayıt
                  </button>
  
           
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
   </section>
    <% } %>